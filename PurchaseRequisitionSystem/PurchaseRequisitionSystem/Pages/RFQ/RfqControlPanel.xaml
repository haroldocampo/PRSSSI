<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="PurchaseRequisitionSystem.RfqControlPanel"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="480" Background="White">

	<Grid x:Name="LayoutRoot">
		<Button x:Name="btnCreate" Style="{DynamicResource Clickable}" Content="Create RFQ" Margin="131,139,358,190" Background="#FF0362BA" FontSize="16" Width="151" Height="151" Click="btnCreate_Click" />
		<Button x:Name="btnUpdate" Style="{DynamicResource Clickable}" Content="Update Sent RFQs" Margin="358,139,131,190" Background="#FF0362BA" FontSize="16" Width="151" Height="151"/>
	</Grid>
</UserControl>